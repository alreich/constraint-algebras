(in-package "CM")

;;; A <overlaps> B
;;;
;;;    AAAA           A = [a1,a2]
;;;      BBBBBB       B = [b1,b2]

;;;                   a1   a2   b1   b2
(setf *OVERLAPS* #N(( :=   :<   :<   :< )
		    ( :>   :=   :>   :< )
		    ( :>   :<   :=   :< )
		    ( :>   :>   :>   := )))



;;; (:< := :<= :> :/= :>= :?)

(setf mat1 #N((:=  :<  :<= :< )
	      (:>  :=  :>  :/=)
	      (:>= :<  :=  :< )
	      (:>  :/= :>  := )))

(propagate mat1)

#N((:=  :<  :<= :< )
   (:>  :=  :>  :/=)
   (:>= :<  :=  :< )
   (:>  :/= :>  := ))


